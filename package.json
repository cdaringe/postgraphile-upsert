{
  "name": "postgraphile-upsert-plugin",
  "description": "add postgres upsert mutations to postgraphile",
  "version": "0.0.0-semantically-released",
  "main": "build/postgraphile-upsert.js",
  "files": [
    "build/*.js",
    "build/*.ts"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "cdaringe <cdaringe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/dockerode": "^2.5.9",
    "@types/execa": "^0.9.0",
    "@types/nanographql": "^2.0.1",
    "@types/pg": "^7.4.11",
    "ava": "1.2.1",
    "bluebird": "^3.5.3",
    "dockerode": "^2.5.7",
    "execa": "^1.0.0",
    "freeport": "^1.0.5",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.3",
    "nanographql": "^2.0.0",
    "node-fetch": "^2.3.0",
    "pg": "^7.6.1",
    "postgraphile": "^4.0.1",
    "postgraphile-core": "4.3.1",
    "prettier-standard": "^9.1.1",
    "typescript": "^3.1.6",
    "semantic-release": "^15.12.1"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier-standard '{src,test,scripts}/**/*.{js,jsx,ts,tsx}'",
    "test": "ava build/**/*.test.js",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{src,test,scripts}/**/*.{js,jsx,ts,tsx}": [
        "yarn format",
        "git add"
      ]
    }
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdaringe/postgraphile-upsert.git"
  },
  "keywords": [
    "postgres", "pg", "postgraphile", "upsert", "plugin", "mutation"
  ]
}
