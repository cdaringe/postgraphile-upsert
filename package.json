{
  "name": "postgraphile-upsert-plugin",
  "description": "add postgres upsert mutations to postgraphile",
  "version": "0.0.0-semantically-released",
  "main": "build/postgraphile-upsert.js",
  "files": [
    "build/*.js",
    "build/*.ts"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "cdaringe <cdaringe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/bluebird": "3.5.36",
    "@types/dockerode": "3.2.7",
    "@types/nanographql": "2.0.1",
    "@types/node": "16.4.13",
    "@types/pg": "8.6.1",
    "@types/wait-on": "5.3.1",
    "@typescript-eslint/parser": "4.29.3",
    "ava": "1.4.1",
    "bluebird": "3.7.2",
    "commitlint-circle": "1.0.0",
    "dockerode": "3.3.0",
    "execa": "5.1.1",
    "freeport": "1.0.5",
    "husky": "7.0.2",
    "nanographql": "2.0.0",
    "node-fetch": "2.6.1",
    "pg": "8.7.1",
    "postgraphile": "4.12.3",
    "postgraphile-core": "4.12.1",
    "prettier-standard": "16.4.1",
    "semantic-release": "17.4.7",
    "standard": "16.0.3",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier-standard '{src,test,scripts}/**/*.{ts,tsx}'",
    "test": "ava build/**/*.test.js",
    "semantic-release": "semantic-release",
    "lint": "standard '{src,test,scripts}/**/*.{ts,tsx}' --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdaringe/postgraphile-upsert.git"
  },
  "keywords": [
    "postgres",
    "pg",
    "postgraphile",
    "upsert",
    "plugin",
    "mutation"
  ],
  "standard": {
    "parser": "@typescript-eslint/parser",
    "standardPlugins": [
      "typescript"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  }
}
