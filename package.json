{
  "name": "postgraphile-upsert-plugin",
  "description": "add postgres upsert mutations to postgraphile",
  "version": "0.0.0-semantically-released",
  "main": "build/postgraphile-upsert.js",
  "files": [
    "build/*.js",
    "build/*.ts"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "cdaringe <cdaringe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/bluebird": "^3.5.24",
    "@types/dockerode": "^2.5.9",
    "@types/nanographql": "^2.0.1",
    "@types/pg": "^7.4.11",
    "@typescript-eslint/parser": "^1.3.0",
    "ava": "1.3.1",
    "bluebird": "^3.5.3",
    "commitlint-circle": "^1.0.0",
    "dockerode": "^2.5.7",
    "execa": "^2.0.3",
    "freeport": "^1.0.5",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "nanographql": "^2.0.0",
    "node-fetch": "^2.3.0",
    "pg": "^7.6.1",
    "postgraphile": "^4.0.1",
    "postgraphile-core": "4.4.4",
    "prettier-standard": "^9.1.1",
    "semantic-release": "^15.12.1",
    "standard": "^13.1.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier-standard '{src,test,scripts}/**/*.{ts,tsx}'",
    "test": "ava build/**/*.test.js",
    "semantic-release": "semantic-release",
    "lint": "standard '{src,test,scripts}/**/*.{ts,tsx}' --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdaringe/postgraphile-upsert.git"
  },
  "keywords": [
    "postgres",
    "pg",
    "postgraphile",
    "upsert",
    "plugin",
    "mutation"
  ],
  "lint-staged": {
    "linters": {
      "{src,test,scripts}/**/*.{ts,tsx}": [
        "yarn format",
        "yarn lint",
        "git add"
      ]
    }
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "standardPlugins": [
      "typescript"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  }
}
