{
  "name": "postgraphile-upsert-plugin",
  "description": "add postgres upsert mutations to postgraphile",
  "version": "0.0.0-semantically-released",
  "main": "build/postgraphile-upsert.js",
  "files": [
    "build/*.js",
    "build/*.ts"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "cdaringe <cdaringe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/bluebird": "3.5.27",
    "@types/dockerode": "2.5.20",
    "@types/nanographql": "2.0.1",
    "@types/pg": "7.4.14",
    "@typescript-eslint/parser": "1.13.0",
    "ava": "1.3.1",
    "bluebird": "3.5.5",
    "commitlint-circle": "1.0.0",
    "dockerode": "2.5.8",
    "execa": "2.0.4",
    "freeport": "1.0.5",
    "husky": "3.0.5",
    "lint-staged": "9.3.0",
    "nanographql": "2.0.0",
    "node-fetch": "2.6.0",
    "pg": "7.11.0",
    "postgraphile": "4.4.1",
    "postgraphile-core": "4.4.4",
    "prettier-standard": "13.0.6",
    "semantic-release": "15.13.18",
    "standard": "12.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier-standard '{src,test,scripts}/**/*.{ts,tsx}'",
    "test": "ava build/**/*.test.js",
    "semantic-release": "semantic-release",
    "lint": "standard '{src,test,scripts}/**/*.{ts,tsx}' --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdaringe/postgraphile-upsert.git"
  },
  "keywords": [
    "postgres",
    "pg",
    "postgraphile",
    "upsert",
    "plugin",
    "mutation"
  ],
  "lint-staged": {
    "linters": {
      "{src,test,scripts}/**/*.{ts,tsx}": [
        "yarn format",
        "yarn lint",
        "git add"
      ]
    }
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "standardPlugins": [
      "typescript"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  }
}
